#pragma once

/* The numeric version format is AAAAABBBBBCCCCCDDDE where:
 *
 * AAAAA - major version number
 * BBBBB - minor version number
 * CCCCC - bugfix version number
 * DDD   - alpha / beta (DDD + 500) version number
 * E     - final (0) / snapshot (1)
 *
 * When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
 *
 * Version      AAAAABBBBBCCCCCDDDE
 *
 * 0.1.0        0000000001000000000
 * 0.1.2        0000000001000020000
 * 1.2.3        0000100002000030000
 * 2.2.0-a.1    0000200001999990010
 * 3.0.0-b.2    0000299999999995020
 * 2.2.0-a.1.z  0000200001999990011
 */
#define LIBCA_CERTIFICATES_CURL_VERSION       $libca_certificates_curl.version.project_number$ULL
#define LIBCA_CERTIFICATES_CURL_VERSION_STR   "$libca_certificates_curl.version.project$"
#define LIBCA_CERTIFICATES_CURL_VERSION_ID    "$libca_certificates_curl.version.project_id$"

#define LIBCA_CERTIFICATES_CURL_VERSION_MAJOR $libca_certificates_curl.version.major$
#define LIBCA_CERTIFICATES_CURL_VERSION_MINOR $libca_certificates_curl.version.minor$
#define LIBCA_CERTIFICATES_CURL_VERSION_PATCH $libca_certificates_curl.version.patch$

#define LIBCA_CERTIFICATES_CURL_PRE_RELEASE   $libca_certificates_curl.version.pre_release$

#define LIBCA_CERTIFICATES_CURL_SNAPSHOT_SN   $libca_certificates_curl.version.snapshot_sn$ULL
#define LIBCA_CERTIFICATES_CURL_SNAPSHOT_ID   "$libca_certificates_curl.version.snapshot_id$"
